  "description": "React Native Elements Next Gen Components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "prepublish": "tsc",
  "files": [
    "dist"
  ],
  "keywords": [
    "react-native",
    "reactjs",
